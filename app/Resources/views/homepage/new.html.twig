{% extends 'PetkoparaTritonCrudBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - New homepage
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>New homepage <span class="glyphicon glyphicon-file" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary"> 
            Submit <span class="glyphicon glyphicon-ok-circle" aria-hidden="true"></span>
        </button>
    </p>


    {{ form_end(form) }}

    <hr/>
        <div class="form-group">
    <a class="btn btn-default" href="{{ path('back_configuration_homepage') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
            </div>

</div>


{% endblock %}

